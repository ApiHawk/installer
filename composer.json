{
    "name": "laravel-zero/installer",
    "description": "The Laravel Zero Installer.",
    "keywords": ["framework", "laravel", "console", "zero"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laravel-zero/installer/issues",
        "source": "https://github.com/laravel-zero/installer"
    },
    "authors": [
        {
            "name": "Nuno Maduro",
            "email": "enunomaduro@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laravel-zero/framework": "dev-hotfix-4.0.25"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "~6.5",
        "mockery/mockery": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "php application app:rename"
        ]
    },
    "bin": ["laravel-zero"]
}
